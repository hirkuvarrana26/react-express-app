{
  "name": "react-express-app",
  "version": "1.0.0",
  "scripts": {
    "client": "npm start --prefix client",
    "server": "nodemon --watch server/src --exec node server/src/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "dependencies": {
    "concurrently": "^7.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}

